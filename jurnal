package fibinacci;

/**
 *
 * @author ADMIN
 */
public class fibonacci_jml{
public static void main(String[] args) {
    int jml=0;
        int febo = 7;
        int[] feb = new int[febo];
        feb[0]=1; 
        feb[1]=1;
        for(int i=2; i<febo; i++){
            feb[i]=feb[i-1]+feb[i-2];
        }
        for(int i=0; i<febo; i++){
            jml=jml+feb[i];
            System.out.println(feb[i]+" ");
        }
        System.out.println("hasil: " +jml);
    }
   
}
