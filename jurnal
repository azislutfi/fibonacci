package fibinacci;

/**
 *
 * @author ADMIN
 */
public class Fibinacci {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int febo = 7;
        int[] feb = new int[febo];
        feb[1]=1; 
        feb[2]=2;
        for(int i=3; i<febo; i++){
            feb[i]=feb[i-1]+feb[i-2];
        }
        for(int i=1; i<febo; i++){
            System.out.println(feb[i] + " ");
        }
    }
